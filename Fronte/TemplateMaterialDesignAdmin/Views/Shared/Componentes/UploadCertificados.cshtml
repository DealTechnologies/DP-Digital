@model TemplateMaterialDesignAdmin.Models.Candidato.Certificado;
<h2>
    Importar Certificados
</h2>
<form asp-controller="Candidato" asp-action="AddCertificados" id="frmFileUpload" class="dropzone" method="post" enctype="multipart/form-data">
    <div class="dz-message">
        <div class="drag-icon-cph">
            <i class="material-icons">touch_app</i>
        </div>
        <h3>Inserir certificados.</h3>
        <em>(Pode inserir multiplos arquivos do tipo pdf, png, jpeg)</em>
    </div>
    <div class="fallback">
        <input name="file" type="file" multiple />
    </div>
</form>
<div>
    <div class="container">
        @if (ViewData["Erro"] != null)
        {
            <p class="error-message">@ViewData["Erro"]</p>
        }
    </div>
    <div class="container">
        @if (ViewData["Resultado"] != null)
        {
            <p class="dz-message">@ViewData["Resuldato"]</p>
        }
    </div>
</div>