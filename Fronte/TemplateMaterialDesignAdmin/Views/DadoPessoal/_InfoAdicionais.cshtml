<div class="row clearfix">
    <div class="col-sm-4">
        <h4>Redes Sociais</h4>
    </div>
</div>
<div class="row clearfix">
    <div class="col-sm-4">
        <label>Linkedin</label>
        <div class="form-group">
            <label for="fname">http://linkedin.com/in/</label>
            <input type="text" id="DMIALinkedin" name="fname" placeholder="seu usuário">
        </div>
    </div>
    <div class="col-sm-4">
        <label>Facebook</label>
        <div class="form-group">
            <label for="fname">http://facebook.com/</label>
            <input type="text"  id="DMIAFacebook" name="fname" placeholder="seu usuário">
        </div>
    </div>
    <div class="col-sm-4">
        <label>Como conheceu a empresa</label>
        <div class="form-group" >
            <select class="form-control show-tick">
                <option value="" id="DMIAComoConheceuEmpresa">Selecione</option>
                <option value="Anuncio">Anúncio</option>
                <option value="Radio">Rádio</option>
                <option value="Televisao">Televisão</option>
                <option value="Email">Email</option>
                <option value="RedesSociais">Redes Sociais</option>
                <option value="SitesdeBusca">Sites de Busca</option>
                <option value="Indicacao">Indicação</option>
                <option value="Prefeitura">Prefeitura</option>
            </select>
        </div>
    </div>
</div>

<hr />

<div class="row clearfix">
    <div class="col-sm-3">
        <h4>Dados físicos</h4>
    </div>
</div>
<div class="row clearfix">
    <div class="col-sm-3">
        <label>Altura</label>
        <div class="form-group">
            <div class="form-line">
                <input type="text" id="DMIAAltura" class="form-control" placeholder="ex: 1,88">
            </div>
        </div>
    </div>
    <div class="col-sm-3">
        <label>Tamanho camisa</label>
        <div class="form-group">
            <div class="form-line">
                <input type="text" id="DMIATamanhoCamisa" class="form-control" placeholder="ex: G">
            </div>
        </div>
    </div>
    <div class="col-sm-3">
        <label>Tamanho calça</label>
        <div class="form-group">
            <div class="form-line">
                <input type="text" id="DMIATamanhoCalca" class="form-control" placeholder="ex: 44">
            </div>
        </div>
    </div>
    <div class="col-sm-3">
        <label>Tamanho calçado</label>
        <div class="form-group">
            <div class="form-line">
                <input type="text" id="DMIATamanhoCalcado" class="form-control" placeholder="ex: 46">
            </div>
        </div>
    </div>
</div>